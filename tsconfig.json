{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "plugins": [
        {
          "name": "next"
        }
      ],
      "paths": {
        "@/*": ["fronted/src/*"]
      }
    },
    "include": ["fronted/next-env.d.ts", "fronted/**/*.ts", "fronted/**/*.tsx", "fronted/.next/types/**/*.ts"],
    "exclude": ["fronted/node_modules"]
  }